{
  "checkpointable": false,
  "name": "{{ app.name }}",
  "executionSystem": "hpc-tacc-wrangler-{{ agave.username }}",
  "executionType": "HPC",
  "deploymentPath": "{{ username }}/apps/{{ app.name }}-{{ app.version }}",
  "deploymentSystem": "{{ app.deployment_system }}",
  "helpURI": "",
  "label": "{{ app.label }}",
  "shortDescription": "{{ app.description }}",
  "longDescription": "",
  "modules": [
    "load tacc-singularity"
  ],
  "ontology": [],
  "parallelism": "SERIAL",
  "tags": [],
  "templatePath": "runner.sh",
  "testPath": "tester.sh",
  "version": "{{ app.version }}",
  "defaultMaxRunTime": "00:30:00",
  "inputs": [
    {
      "id": "experiment_dir",
      "details": {
        "label": "Experiment Directory",
        "description": "The directory the design app will use to source the initial experiment files."
      },
      "semantics": {}
    },
    {
      "id": "state_json",
      "details": {
        "label": "State Json"
      },
      "semantics": {}
    },
    {
      "id": "lab_configuration_uri",
      "details": {
        "label": "Lab Configuration URI"
      },
      "semantics": {},
      "value": {
        "required": false,
        "type": "string"
      }
    }
  ],
  "parameters": [
    {
      "id": "out_path",
      "details": {
        "label": "Output Path",
        "description": "The directory the design app will use to output the resulting experiment files."
      },
      "semantics": {},
      "value": {
        "visible": true,
        "required": true,
        "type": "string"
      }
    },
    {
      "id": "experiment_id",
      "details": {
        "label": "Experiment ID"
      },
      "semantics": {},
      "value": {
        "visible": true,
        "required": true,
        "type": "string"
      }
    },
    {
      "id": "lab_configuration",
      "details": {
        "label": "Lab Configuration"
      },
      "semantics": {},
      "value": {
        "visible": true,
        "required": false,
        "type": "string"
      }
    },
    {
      "id": "challenge_problem",
      "details": {
        "label": "Challenge Problem"
      },
      "semantics": {},
      "value": {
        "visible": true,
        "required": true,
        "type": "string"
      }
    },
    {
      "id": "CONTAINER_IMAGE",
      "value": {
        "default": "{{ docker.namespace }}/{{ docker.repo }}:{{ docker.tag }}",
        "type": "string",
        "visible": false,
        "required": true,
        "order": 1000
      }
    }
  ],
  "outputs": []
}